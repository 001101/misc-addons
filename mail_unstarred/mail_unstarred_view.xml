<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="assets_backend_inherited_mail_unstarred" name="Mail unstarred" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/mail_unstarred/static/src/js/mail_unstarred.js"></script>
            </xpath>
        </template>

        <record id="mail.action_mail_star_feeds" model="ir.actions.client">
            <field name="context">
                {
                    'default_model': 'res.users',
                    'default_res_id': uid,
                    'thread_model': 'res.partner',
                    'search_default_message_unread': False,
                    'view_todo': True
                }
            </field>
        </record>


     </data>
</openerp>
